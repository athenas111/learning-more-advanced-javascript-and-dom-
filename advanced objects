function HouseKeeper(name, age, experience, periodOfWork) { //'constructor' function needs to be capitalized
    this.name = name;
    this.age = age;
    this.experience = experience;
    this.periodOfWork = periodOfWork;
}

var houseKeeper1 = new HouseKeeper('gabriela', 18, ["cleaning", "cooking"], 'night period');


console.log(houseKeeper1);
